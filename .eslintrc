{
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "semi": ["error", "always"],
        "no-console": "off",
        "no-extra-boolean-cast": 0,
        "no-unused-vars": 0
    }
}
